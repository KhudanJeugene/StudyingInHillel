import static org.junit.Assert.*;

import org.junit.Test;

public class HW9_2Test
{
	@Test
	public void testIntToString_1()
	{
		int n = 1;
		String res = HW9_2.intToString(n);
		assertEquals("1",res);
	}
	@Test
	public void testIntToString_0()
	{
		int n = 0;
		String res = HW9_2.intToString(n);
		assertEquals("0",res);
	}
	@Test
	public void testIntToString()
	{
		int n = -1;
		String res = HW9_2.intToString(n);
		assertEquals("-1",res);
	}
}
