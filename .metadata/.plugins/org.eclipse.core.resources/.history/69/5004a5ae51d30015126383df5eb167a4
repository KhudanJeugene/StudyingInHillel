	@Test
	public void testNumberConvert_1()
	{
		long numb = -1;
		String exp = "минус один";
		String act = HW4.numberConvert(numb);
		assertEquals(exp,act);
	}
	@Test
	public void testNumberConvert_0()
	{
		long numb = 0;
		String exp = "ноль";
		String act = HW4.numberConvert(numb);
		assertEquals(exp,act);
	}
	@Test
