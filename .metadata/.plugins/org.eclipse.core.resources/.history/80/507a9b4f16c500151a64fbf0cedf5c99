package withMain;

public class IntToString 
{
	public static String intToString(int i) throws Exception
	{
		String res = "";
		try
		{
		res = String.valueOf(i);
		}
		catch(NumberFormatException e)
		{
			System.out.println(e.getStackTrace());
		}
		return res;
	}
	public static void main(String[] args) 
	{
		try
		{
		String res = intToString(ð);
		}
		catch(Exception e)
		{
			e.printStackTrace();
		}
		
	}

}
